[user]
	  name = Christopher Pappas
	  email = damassi.pappas@gmail.com
[color]
    diff = auto
    status = auto
    branch = auto
    interactive = auto
    ui = true
    pager = true
[core]
    excludesfile = ~/.gitignore_global
    whitespace = trailing-space,space-before-tab
[branch]
    autosetuprebase = always
    autosetupmerge = false
[push]
    default = upstream
[credential]
	  helper = osxkeychain
[alias]
  st = status
  ci = commit
  br = branch
  co = checkout
  df = diff
  lg = log -p


